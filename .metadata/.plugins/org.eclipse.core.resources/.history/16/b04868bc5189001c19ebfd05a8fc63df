package week2_2;

//
public class Student {

	public static void main(String[] args) {
		int id,grade,sub1,sub2,sub3;
		String name;
		id=1;
		grade=10;
		sub1=80;
		sub2=70;
		sub3=60;
		name="Ram";
		System.out.printf("Id=%d\t Name=%s\t Grade=%d\n Marks Obtained:- Computer=%d Database=%d Networking=%d",id,name,grade,sub1,sub2,sub3);

	}

}
