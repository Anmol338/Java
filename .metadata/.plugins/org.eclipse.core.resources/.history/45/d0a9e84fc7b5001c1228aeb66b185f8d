package mylibs;

// Registration Class
public class Registration {

	// Initial Variables
	String fname, lname, gender, address, postcode, email, contact, creditcard, password, cpassword;

	public Registration(String fname, String lname, String gender, String address, String postcode, String email,
			String contact, String creditcard, String password, String cpassword) {
		this.fname = fname;
		this.lname = lname;
		this.gender = gender;
		this.address = address;
		this.postcode = postcode;
		this.email = email;
		this.contact = contact;
		this.creditcard = creditcard;
		this.password = password;
		this.cpassword = cpassword;
	}
	
	

}
