package mylibs;

public class Room {
	int id;
	String type;
	double rate;
	String status;
	
	public Room() {
		this.id = 0;
		this.type = "";
		this.rate = 0.0;
		this.status = "";
	}
	
	public Room(int id, String type, double rate, String status) {
		this.id = id;
		this.type = type;
		this.rate = rate;
		this.status = status;
	}
	
	
}
